{
  "name": "next-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "storybook": "pnpm --filter @acme/core storybook",
    "storybook:build": "pnpm --filter @acme/core storybook:build"
  },
  "devDependencies": {
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.4",
    "eslint-plugin-react": "7.35.0",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "typescript": "5.6.3",
    "sass": "1.77.6",
    "@types/node": "^20.17.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.2",
    "turbo": "2.6.1"
  },
  "pnpm": {
    "overrides": {
      "clsx": "2.1.1",
      "next": "16.0.5",
      "react": "19.2.0",
      "react-dom": "19.2.0"
    }
  },
  "packageManager": "pnpm@10.11.0",
  "volta": {
    "node": "20.19.1"
  }
}
